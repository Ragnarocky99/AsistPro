	<parameter name="idAsistencia" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    alumno.nombre AS nombre_alumno,
    alumno.apellido AS apellido_alumno,
    alumno.curso AS curso_alumno,
    alumno.seccion AS seccion_alumno,
    asistencia.fecha AS fecha_asistencia,
    materia.nombre AS nombre_materia,
    horario.hora_inicio AS hora_inicio_clase,
    horario.hora_fin AS hora_fin_clase,
    profesor.nombre AS nombre_profesor,
    profesor.apellido AS apellido_profesor,
    especialidad.nombre AS nombre_especialidad,
    CASE 
        WHEN detalle_asistencia.esta_presente = TRUE THEN 'SI'
        ELSE 'NO'
    END AS presente,
    CASE 
        WHEN detalle_asistencia.hora_presencia IS NOT NULL THEN detalle_asistencia.hora_presencia
        ELSE '-'
    END AS hora_presencia,
    detalle_asistencia.rasgos AS rasgos
FROM 
    ctn_db.detalle_asistencia AS detalle_asistencia
INNER JOIN 
    ctn_db.asistencia AS asistencia 
    ON detalle_asistencia.id_asistencia = asistencia.id_asistencia
INNER JOIN 
    ctn_db.horario AS horario 
    ON asistencia.id_horario = horario.id_horario
INNER JOIN 
    ctn_db.materia AS materia 
    ON horario.id_materia = materia.id_materia
INNER JOIN 
    ctn_db.profesor AS profesor 
    ON horario.id_profesor = profesor.id_profesor
INNER JOIN 
    ctn_db.alumno AS alumno 
    ON detalle_asistencia.id_alumno = alumno.id_alumno
INNER JOIN 
    ctn_db.especialidad AS especialidad 
    ON horario.id_especialidad = especialidad.id_especialidad 
    AND alumno.id_especialidad = especialidad.id_especialidad;
]]>
